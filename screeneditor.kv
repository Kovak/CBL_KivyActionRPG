#:kivy 1.3.0


<Tile>:
    size: root.size
    source_image: root.source_image
    Image:
        source: root.atlas + 'Tile-' + (root.interior if root.tile_type == 'whole_no_anim' else (root.interior + '-' + root.exterior + '-Position' + root.position_code if root.tile_type == 'partial_no_anim' else (root.interior + '-' + root.exterior + '-Position' + root.position_code + '-' + str(root.parent.active_frame % root.frames + 1) if root.tile_type == 'partial_anim' else root.interior + '-' + str(root.parent.active_frame % root.frames + 1))))
        size: root.size
        pos: root.pos
        mipmap: True